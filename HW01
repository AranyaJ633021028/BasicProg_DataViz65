      "source": [
        "def time_sec(T1,T2):\n",
        "  sp_T1 = T1.split(':')\n",
        "  sp_T2 = T2.split(':')\n",
        "  a = (int(sp_T1[0])*60*60)+(int(sp_T1[1])*60)+int(sp_T1[2])\n",
        "  b = (int(sp_T2[0])*60*60)+(int(sp_T2[1])*60)+int(sp_T2[2])\n",
        "  y = b-a\n",
        "  return y\n",
        "\n",
        "  print('done!')\n"
      ]
    },
    {
